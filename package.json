  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "test:unit": "jest",
    "test:e2e": "npx cypress run --e2e",
    "test:e2e:open": "npx cypress open --e2e --browser chrome",
    "lint": "eslint",
    "format": "prettier --write \"{src,public,cypress}/**/*.{js,jsx,ts,tsx,css,json,html}\"",
